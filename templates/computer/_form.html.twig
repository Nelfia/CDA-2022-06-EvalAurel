<div class="container">
    {{ form_start(form) }}
        <div class="col">{{ form_row(form.model) }}</div>
        <div class="col">{{ form_row(form.description) }}</div>
        <div class="col">{{ form_row(form.price) }}</div>
        <div class="col">{{ form_row(form.year) }}</div>
        <div class="col">{{ form_row(form.sin) }}</div>
        <div class="col">{{ form_row(form.img) }}</div>
        <div class="col">{{ form_row(form.brand) }}</div>

        <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}
</div>
